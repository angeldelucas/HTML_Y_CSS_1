{
   "extends": [
      "stylelint-config-clean-order/error"
   ],
   "rules": {
      "order/properties-order": [
         "width",
         "height",
         "padding",
         "padding-top",
         "padding-right",
         "padding-bottom",
         "padding-left",
         "border",
         "margin",
         "margin-top",
         "margin-right",
         "margin-bottom",
         "margin-left",
         "display",
         "flex-direction",
         "position",
         "top",
         "right",
         "bottom",
         "left"
      ]
   },
   "plugins": [
      "stylelint-order"
   ]
}